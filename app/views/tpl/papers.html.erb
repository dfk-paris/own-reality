<main class="page-content clearfix" role="main">
  <div class="region region-content">
    <div id="#block-dfk-content" class="block block-system block-system-main-block">
      <div class="department">

        <h2 class="department-main-title"></h2>

        <div class="department-content">

          <div ng-controller="papers_controller">
            <ul>
              <li ng-repeat="o in objects">
                <span
                  ng-bind="o._source.id"
                  ng-show="debug()"
                  class="text-muted"
                ></span>
                <a
                  ng-show="o._source.html"
                  or-bind-translate="o._source.title"
                  href="#/papers/{{o._type}}/{{o._source.id}}"
                ></a>
                <span
                  ng-hide="o._source.html"
                  or-bind-translate="o._source.title"
                ></span>
              </li>
            </ul>
          </div>

        </div>

      </div>
    </div>
  </div>
</main>